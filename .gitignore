# Ignore environment configuration
.env
.env.example

# Ignore cache and logs
bootstrap/cache/
storage/*.log
storage/logs/
storage/framework/*
storage/app/public/*

# Ignore vendor and node_modules
/vendor
/node_modules

# Ignore IDE/editor files
.idea/
.vscode/
*.sublime-project
*.sublime-workspace

# Ignore system files
.DS_Store
Thumbs.db

